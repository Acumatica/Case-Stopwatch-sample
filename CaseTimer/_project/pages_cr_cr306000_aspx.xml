                    <Prop Key="IsStartupScript" Value="True" />
                    <Prop Key="Script" Value="        var tID = null;          function Tick() {             Number.prototype.format = window.top.Number.prototype.format;             var CstButton5 = px_all[&quot;ctl00_phF_form_CstButton5&quot;];             var c = CstButton5.getText();             var arr = c.split(&quot;:&quot;);             var seconds = Number(arr[2]) + 1;             var minutes = Number(arr[1]);             var hours = Number(arr[0]);              if (seconds == 60) {                 seconds = 0;                 minutes = minutes + 1;             }              if (minutes == 60) {                 minutes = 0;                 hours = hours + 1;             }              CstButton5.setText([hours.format(&quot;d2&quot;), minutes.format(&quot;d2&quot;), seconds.format(&quot;d2&quot;)].join(&quot;:&quot;));             var edUsrClockTick = px_all[&quot;ctl00_phF_form_edUsrClockTick&quot;];             edUsrClockTick.updateValue(CstButton5.getText());         }          function buttonClick(sender) {             if (tID != null) {                 var edUsrClockTick = px_all[&quot;ctl00_phF_form_edUsrClockTick&quot;];                 var CstButton5 = px_all[&quot;ctl00_phF_form_CstButton5&quot;];                  edUsrClockTick.updateValue(CstButton5.getText());                  window.clearInterval(tID);                 tID = null;             }             else {                 tID = window.setInterval(function () {                     Tick();                 }, 1000);             }         }          function CommandPerformed(ds, context) {             var edUsrClockTick = px_all[&quot;ctl00_phF_form_edUsrClockTick&quot;];             var CstButton5 = px_all[&quot;ctl00_phF_form_CstButton5&quot;];              if (context.command == &quot;Save&quot;) {                 edUsrClockTick.setValue(CstButton5.getText());                 window.clearInterval(tID);                 tID = null;             }             else if (context.command == &quot;ReloadPage&quot; || context.command == &quot;Cancel&quot; ||                 context.command == &quot;Next&quot; || context.command == &quot;Previous&quot; || context.command == &quot;First&quot; || context.command == &quot;Last&quot;) {                  var timerStr = edUsrClockTick.getValue();                 if (timerStr == null)                     timerStr = &quot;00:00:00&quot;;                  CstButton5.setText(timerStr);             }         }" />
